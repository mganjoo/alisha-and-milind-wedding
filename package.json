  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "start": "npm run develop",
    "start:prod": "npm run build && npm run serve",
    "clean": "gatsby clean",
    "test": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit && npm run test:e2e:ci",
    "pretest:ci": "npm run check",
    "test:unit": "jest",
    "test:e2e": "server-test develop 8000 cy:run",
    "test:e2e:open": "server-test develop 8000 cy:open",
    "test:e2e:ci": "CYPRESS_baseUrl=http://localhost:9000 server-test start:prod 9000 cy:run:ci",
    "check": "npm run check:types && npm run lint",
    "check:types": "tsc",
    "lint": "eslint --cache --ignore-path .gitignore . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "storybook": "start-storybook -p 6006",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "percy exec -- npm run cy:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mganjoo/alisha-and-milind-wedding"
  },
  "bugs": {
    "url": "https://github.com/mganjoo/alisha-and-milind-wedding/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
