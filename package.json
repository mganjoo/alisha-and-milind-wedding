{
  "name": "alisha-and-milind-wedding",
  "private": true,
  "description": "Website for Alisha and Milind's wedding",
  "version": "0.1.0",
  "author": "Milind Ganjoo <milind.ganjoo@gmail.com>",
  "dependencies": {
    "gatsby": "^2.15.11",
    "gatsby-image": "^2.2.17",
    "gatsby-plugin-manifest": "^2.2.15",
    "gatsby-plugin-offline": "^2.2.9",
    "gatsby-plugin-postcss": "^2.1.4",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.1.7",
    "gatsby-plugin-remove-fingerprints": "0.0.2",
    "gatsby-plugin-sharp": "^2.2.16",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-sharp": "^2.2.13",
    "mem": ">=4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "tailwindcss": "^1.1.2",
    "typeface-eb-garamond": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typeface-playfair-display": "0.0.72"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/cypress": "^4.2.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "axe-core": "^3.3.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-gatsby": "^0.2.13",
    "cross-env": "^5.2.1",
    "cypress": "^3.4.1",
    "cypress-axe": "^0.5.1",
    "cypress-image-snapshot": "^3.1.1",
    "cypress-multi-reporters": "^1.2.1",
    "husky": "^3.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "mocha": "^6.2.0",
    "mocha-junit-reporter": "^1.23.1",
    "postcss-nesting": "^7.0.1",
    "prettier": "^1.18.2",
    "start-server-and-test": "^1.10.0",
    "yn": "^3.1.1"
  },
  "keywords": [
    "gatsby",
    "wedding"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "test": "npm run test:unit && npm run test:e2e && npm run test:e2e:guard",
    "pretest": "npm run cy:clean",
    "test:ci": "npm run test:unit && npm run test:e2e:ci && npm run test:e2e:guard",
    "pretest:ci": "npm run cy:clean",
    "test:unit": "jest",
    "test:e2e": "server-test develop http://localhost:8000 cy:run",
    "test:e2e:ci": "server-test develop http://localhost:8000 cy:run:ci",
    "test:e2e:guard": "cross-env GUARD_PAGE_ONLY=1 npm run test:e2e",
    "pretest:e2e:guard": "npm run clean",
    "test:e2e:open": "server-test develop http://localhost:8000 cy:open",
    "test:e2e:screenshots:update": "server-test develop http://localhost:8000 cy:screenshots:update",
    "storybook": "start-storybook -p 6006",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --env relaxThresholds=true --reporter cypress-multi-reporters --reporter-options configFile=cypress-reporters.json",
    "cy:screenshots:update": "cypress run --env updateSnapshots=true",
    "cy:clean": "rm -rf cypress/diffs/* cypress/results/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mganjoo/alisha-and-milind-wedding"
  },
  "bugs": {
    "url": "https://github.com/mganjoo/alisha-and-milind-wedding/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
