{
  "name": "alisha-and-milind-wedding",
  "private": true,
  "description": "Website for Alisha Saxena and Milind Ganjoo's wedding",
  "version": "0.1.0",
  "author": "Milind Ganjoo <milind.ganjoo@gmail.com>",
  "dependencies": {
    "gatsby": "^2.13.76",
    "gatsby-image": "^2.2.12",
    "gatsby-plugin-manifest": "^2.2.8",
    "gatsby-plugin-offline": "^2.2.9",
    "gatsby-plugin-postcss": "^2.1.4",
    "gatsby-plugin-purgecss": "^4.0.0",
    "gatsby-plugin-react-helmet": "^3.1.4",
    "gatsby-plugin-sharp": "^2.2.16",
    "gatsby-source-filesystem": "^2.1.14",
    "gatsby-transformer-sharp": "^2.2.10",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1",
    "tailwindcss": "^1.1.2",
    "typeface-eb-garamond": "0.0.72",
    "typeface-montserrat": "0.0.75",
    "typeface-playfair-display": "0.0.72"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/addon-storyshots-puppeteer": "^5.1.11",
    "@storybook/addon-viewport": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-preset-gatsby": "^0.2.11",
    "husky": "^3.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-image-snapshot": "^2.9.0",
    "jest-puppeteer": "^4.3.0",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "puppeteer": "^1.19.0",
    "react-test-renderer": "^16.9.0",
    "start-server-and-test": "^1.10.0"
  },
  "keywords": [
    "gatsby",
    "wedding"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "npm run test:unit && npm run test:ui",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook:ui": "start-storybook -p 9009 --ci",
    "test:unit": "jest",
    "test:ui": "server-test storybook:ui http-get://localhost:9009 'jest -c jest-stories.config.js'",
    "test:ui-update": "server-test storybook:ui http-get://localhost:9009 'jest -c jest-stories.config.js j-u'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mganjoo/alisha-and-milind-wedding"
  },
  "bugs": {
    "url": "https://github.com/mganjoo/alisha-and-milind-wedding/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
