{
  "name": "@alisha-and-milind-wedding/cli",
  "description": "Admin tool to manage wedding data",
  "private": true,
  "version": "0.1.0",
  "bin": {
    "wedding-manager": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "1.8.16",
    "@oclif/config": "1.18.2",
    "@oclif/plugin-help": "5.1.10",
    "chalk": "4.1.2",
    "cli-ux": "6.0.6",
    "dayjs": "1.10.7",
    "firebase-admin": "10.0.1",
    "fs-extra": "10.0.0",
    "google-auth-library": "7.11.0",
    "googleapis": "92.0.0",
    "lodash": "4.17.21",
    "mailchimp-api-v3": "1.15.0",
    "papaparse": "5.3.1",
    "shortid": "2.2.16",
    "tslib": "2.3.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.10",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "27.0.3",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.15",
    "@types/papaparse": "5.3.1",
    "@types/shortid": "0.0.29",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "jest": "27.4.5",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wedding-manager",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "contacts": {
        "description": "Export/sync contacts collection"
      },
      "invite": {
        "description": "Manage invitations"
      }
    }
  },
  "scripts": {
    "test": "jest",
    "test:ci": "npm run check:types && npm run check:lint && npm run test",
    "readme": "oclif-dev readme && git add README.md",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint --cache --ignore-path .gitignore . --ext ts --max-warnings 0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "resetMocks": true
  }
}
