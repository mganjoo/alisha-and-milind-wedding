{
  "name": "@alisha-and-milind-wedding/cli",
  "description": "Admin tool to manage wedding data",
  "private": true,
  "version": "0.1.0",
  "bin": {
    "wedding-manager": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.14.0",
    "@oclif/plugin-help": "2.2.3",
    "chalk": "3.0.0",
    "cli-ux": "5.4.5",
    "dayjs": "1.8.23",
    "firebase-admin": "8.10.0",
    "fs-extra": "8.1.0",
    "googleapis": "47.0.0",
    "lodash": "4.17.15",
    "mailchimp-api-v3": "1.13.1",
    "papaparse": "5.1.1",
    "shortid": "2.2.15",
    "tslib": "1.11.1",
    "yn": "4.0.0",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/fs-extra": "8.1.0",
    "@types/jest": "24.9.1",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.32",
    "@types/papaparse": "5.0.3",
    "@types/shortid": "0.0.29",
    "@types/yup": "0.26.33",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "jest": "25.2.4",
    "ts-jest": "25.3.0",
    "ts-node": "8.8.1",
    "typescript": "3.8.3"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wedding-manager",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "contacts": {
        "description": "Export/sync contacts collection"
      },
      "invite": {
        "description": "Manage invitations"
      }
    }
  },
  "scripts": {
    "test": "jest",
    "test:ci": "npm run check:types && npm run check:lint && npm run test",
    "readme": "oclif-dev readme && git add README.md",
    "check:types": "tsc --noEmit",
    "check:lint": "eslint --cache --ignore-path .gitignore . --ext ts --max-warnings 0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "resetMocks": true
  }
}
