{
  "name": "cli",
  "description": "Admin tool to manage wedding data",
  "private": true,
  "version": "0.1.0",
  "author": "Milind Ganjoo <milind.ganjoo@gmail.com>",
  "bin": {
    "wedding-manager": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "@types/shortid": "0.0.29",
    "ajv": "^6.10.2",
    "chalk": "^2.4.2",
    "cli-ux": "^5.3.2",
    "dayjs": "^1.8.16",
    "firebase-admin": "^8.6.1",
    "shortid": "^2.2.15",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/node": "^12.11.1",
    "ajv-cli": "^3.0.0",
    "globby": "^10.0.1",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wedding-manager",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md",
    "fixtures:validate": "! ls ../fixtures/*.json >/dev/null 2>&1 || ajv -s fixture-schema.json -d ../fixtures/*.json",
    "load:invitations:emulator": "FIRESTORE_EMULATOR_HOST=localhost:8080 ./bin/run load-fixtures ../fixtures/invitations.json",
    "check:types": "tsc --noEmit"
  },
  "types": "lib/index.d.ts"
}
