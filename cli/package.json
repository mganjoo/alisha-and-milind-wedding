{
  "name": "@alisha-and-milind-wedding/cli",
  "description": "Admin tool to manage wedding data",
  "private": true,
  "version": "0.1.0",
  "bin": {
    "wedding-manager": "./bin/run"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-help": "2.2.3",
    "@types/shortid": "0.0.29",
    "ajv": "6.10.2",
    "chalk": "3.0.0",
    "cli-ux": "5.3.3",
    "dayjs": "1.8.18",
    "firebase-admin": "8.9.0",
    "shortid": "2.2.15",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/node": "12.12.22",
    "ajv-cli": "3.0.0",
    "globby": "10.0.1",
    "ts-node": "8.5.4",
    "typescript": "3.7.3"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wedding-manager",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "test": "npm run check:types",
    "readme": "oclif-dev readme && git add README.md",
    "check:types": "tsc --noEmit"
  }
}
