{
  "files.associations": {
    "global.css": "postcss",
    "netlify.toml": "ini"
  },
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "npm.includeDirectories": ["website", "cli"],
  "eslint.workingDirectories": ["website", "cli"],
  "json.schemas": [
    {
      "fileMatch": ["cli/fixtures/*.json"],
      "url": "cli/fixture-schema.json"
    }
  ]
}
